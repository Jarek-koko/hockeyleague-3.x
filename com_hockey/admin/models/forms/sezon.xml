            <option value="7">7</option>
            <option value="8">8</option>
            <option value="9">9</option>
            <option value="10">10</option>   
        </field> 

        <field name="overtime" type="radio"
               class="btn-group"
               label="COM_HOCKEY_FORM_LBL_SEZON_OVERTIME"
               default="F" labelclass="control-label">
            <option value="F">JNO</option>
            <option value="T">JYES</option>
        </field> 
        
        <field name="p_k_w" type="list" class="inputbox"
               label="COM_HOCKEY_FORM_LBL_SEZON_P_K_W"
               default="0">
