            <option value="F">JNO</option>
            <option value="T">JYES</option>
        </field> 
        
       
        <field name="year" type="text" class="inputbox validate-numeric"
               size="4" maxlength="4" 
               label="COM_HOCKEY_FORM_LBL_SEZON_YEAR"
               filter="safehtml" /> 

        <field name="state" type="list"
               label="JSTATUS"
               class="inputbox small"
               size="1"
               default="1">
            <option value="1">JPUBLISHED</option>
            <option value="0">JUNPUBLISHED</option>
            <option value="2">JARCHIVED</option>
            <option value="-2">JTRASHED</option>
        </field>

        <field name="ordering" type="Ordering" class="inputbox"
               label="JFIELD_ORDERING_LABEL"
        />

        <field name="created" type="calendar"
               label="JGLOBAL_FIELD_CREATED_LABEL" 
               class="inputbox" size="22" format="%Y-%m-%d %H:%M:%S"
               filter="user_utc" />

        <field name="created_by" type="user"
               label="JGLOBAL_FIELD_CREATED_BY_LABEL" 
        />

        <field name="modified" type="calendar" 
               class="readonly"
               label="JGLOBAL_FIELD_MODIFIED_LABEL" 
               size="22" readonly="true" format="%Y-%m-%d %H:%M:%S"
               filter="user_utc" />

        <field name="modified_by" type="user"
               label="JGLOBAL_FIELD_MODIFIED_BY_LABEL"
               class="readonly"
               readonly="true"
               filter="unset"  />

    </fieldset>

